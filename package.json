{
	"scripts": {
		"copy:config": "cpx 'config/**/*.yaml' dist/config/",
		"dev:ts": "cross-env NODE_ENV=development concurrently \" pnpm build:ts\" \"tsc -w\" \"ts-node-dev -r tsconfig-paths/register --transpile-only src/server.ts | pino-pretty --colorize\"",
		"dev:js": "cross-env NODE_ENV=development concurrently \"tsc -w\" \" pnpm build:ts && nodemon dist/server.js\"",
		"build:ts": "rimraf dist && tsc -p tsconfig.json && tsc-alias && pnpm copy:config",
		"webpack": "cross-env NODE_ENV=production pnpm build:ts && webpack --config webpack.config.ts",
		"prisma:init": "dotenv -e .env.development pnpm exec prisma init && rimraf .env",
		"prisma:dev": "dotenv -e .env.development pnpm exec prisma db pull && dotenv -e .env.development pnpm exec prisma generate",
		"prisma:deploy": "dotenv -e .env.production pnpm exec prisma db pull && dotenv -e .env.production pnpm exec prisma generate",
		"prisma:migrate": "dotenv -e .env.development pnpm exec prisma migrate dev --name init",
		"prisma:reset": "dotenv -e .env.development pnpm exec prisma migrate reset",
		"prisma:validate": "dotenv -e .env.development pnpm exec prisma validate",
		"manualCreateUser": "dotenv -e .env.development ts-node-dev src/controllers/addUser.ts",
		"postinstall": "pnpm approve-builds",
		"start:test": "cross-env NODE_ENV=production node bin/server.js",
		"start:prod": "pm2 start bin/ecosystem.config.js --env production"
	},
